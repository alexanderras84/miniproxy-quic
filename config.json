{
  "log": {
    "level": "info",
    "timestamp": true
  },
  "inbounds": [
    {
      "type": "tproxy",
      "tag": "tproxy-tcp",
      "listen": "0.0.0.0",
      "listen_port": 15001,
      "network": "tcp",
      "sniff": {
        "enabled": true,
        "dest_override": ["http", "tls"]
      }
    },
    {
      "type": "tproxy",
      "tag": "tproxy-udp",
      "listen": "0.0.0.0",
      "listen_port": 15001,
      "network": "udp"
    }
  ],
  "outbounds": [
    {
      "type": "direct",
      "tag": "direct"
    },
    {
      "type": "block",
      "tag": "blocked"
    }
  ],
  "route": {
    "rules": [
      {
        "type": "field",
        "ip": [
          "geoip:private"
        ],
        "outbound": "direct"
      },
      {
        "type": "field",
        "inbound": ["tproxy-tcp", "tproxy-udp"],
        "ip": [],
        "outbound": "blocked"
      }
    ],
    "auto_detect_interface": true
  }
}