{
  "log": {
    "level": "info",
    "timestamp": true
  },
  "inbounds": [
    {
      "type": "tproxy",
      "tag": "tproxy-tcp",
      "listen": "0.0.0.0",
      "listen_port": 15001,
      "network": "tcp",
      "sniff": {
        "enabled": true,
        "dest_override": ["http", "tls", "quic"],
        "metadata_only": false
      }
    },
    {
      "type": "tproxy",
      "tag": "tproxy-udp",
      "listen": "0.0.0.0",
      "listen_port": 15001,
      "network": "udp",
      "sniff": {
        "enabled": true,
        "dest_override": ["quic"],
        "metadata_only": false
      }
    }
  ],
  "outbounds": [
    {
      "type": "direct",
      "tag": "direct"
    }
  ],
  "route": {
    "rules": [
      {
        "type": "field",
        "ip": ["geoip:private"],
        "outbound": "direct"
      }
    ],
    "auto_detect_interface": true
  }
}